     <owl:ObjectProperty rdf:about="&arl;hasActionStatusValue"> <rdfs:label xml:lang="en">has action status value</rdfs:label> <rdfs:comment xml:lang="en">Links an AgentAction to its specific status from the controlled vocabulary.</rdfs:comment> <rdfs:domain rdf:resource="&arl;AgentAction"/> <rdfs:range rdf:resource="&arl;ActionStatusCode"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasHeatLevelValue"> <rdfs:label xml:lang="en">has heat level value</rdfs:label> <rdfs:comment xml:lang="en">Links an Artist to their specific heat level from the controlled vocabulary.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;HeatLevelValue"/> </owl:ObjectProperty>
     <owl:ObjectProperty rdf:about="&arl;hasDeliveryStandardValue"> <rdfs:label xml:lang="en">has delivery standard value</rdfs:label> <rdfs:comment xml:lang="en">Links a DeliveryRequirement to its specific standard from the controlled vocabulary.</rdfs:comment> <rdfs:domain rdf:resource="&arl;DeliveryRequirement"/> <rdfs:range rdf:resource="&arl;DeliveryStandardValue"/> </owl:ObjectProperty>
