<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY arl "http://www.autonomousrecordlabel.com/ontology#" >
]>

<rdf:RDF xmlns="&arl;"
     xml:base="&arl;"
     xmlns:arl="&arl;"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="&arl;">
        <rdfs:label>Autonomous Record Label Ontology</rdfs:label>
        <rdfs:comment>An ontology describing the domain of an AI-powered record label, optimized for agentic AI, RL, and revenue generation. Incorporates insights from Donald Passman's 'All You Need to Know About the Music Business.' Version includes refined properties, class hierarchies, axioms for contracts, marketing, agent actions, inverse object properties, controlled vocabularies, and monetary amount modeling.</rdfs:comment>
        <owl:versionInfo>2.2.2</owl:versionInfo> <!-- Incremented version for changes -->
    </owl:Ontology>

    <!-- Annotation Properties -->
    <owl:AnnotationProperty rdf:about="&arl;sourceSystem"/>
    <owl:AnnotationProperty rdf:about="&arl;confidenceScore"/>
    <owl:AnnotationProperty rdf:about="&arl;lastUpdated"/>
    <owl:AnnotationProperty rdf:about="&arl;potentialRLRewardMetric"/>

    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties (Full Set with Domains, Ranges, Labels, Comments, Inverses)
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <owl:ObjectProperty rdf:about="&arl;hasTeamMember"> <rdfs:label xml:lang="en">has team member</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist or MusicGroup to a professional who is part of their career team (e.g., Manager, Attorney). Passman Ch. 2.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;MusicGroup"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;TeamMember"/> <owl:inverseOf rdf:resource="&arl;isTeamMemberFor"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isTeamMemberFor"> <rdfs:label xml:lang="en">is team member for</rdfs:label> <rdfs:comment xml:lang="en">Relates a TeamMember to the Artist or MusicGroup they provide services for. Inverse of hasTeamMember.</rdfs:comment> <rdfs:domain rdf:resource="&arl;TeamMember"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;MusicGroup"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;hasTeamMember"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;managesCareerOf"> <rdfs:label xml:lang="en">manages career of</rdfs:label> <rdfs:comment xml:lang="en">Relates a PersonalManager to the Artist or MusicGroup whose career they manage. Passman Ch. 3.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PersonalManager"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;MusicGroup"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;careerManagedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;careerManagedBy"> <rdfs:label xml:lang="en">career managed by</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist or MusicGroup to their PersonalManager. Inverse of managesCareerOf. Passman Ch. 3.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;MusicGroup"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;PersonalManager"/> <owl:inverseOf rdf:resource="&arl;managesCareerOf"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;signsArtist"> <rdfs:label xml:lang="en">signs artist</rdfs:label> <rdfs:comment xml:lang="en">Relates a RecordLabel (or MusicPublisher for songwriters) to an Artist (or Songwriter) they sign to a contract.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;RecordLabel"/> <rdf:Description rdf:about="&arl;MusicPublisher"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;Songwriter"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;isSignedTo"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isSignedTo"> <rdfs:label xml:lang="en">is signed to</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist (or Songwriter) to the RecordLabel (or MusicPublisher) they are signed with. Inverse of signsArtist.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Artist"/> <rdf:Description rdf:about="&arl;Songwriter"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;RecordLabel"/> <rdf:Description rdf:about="&arl;MusicPublisher"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;signsArtist"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;specifiesRoyalty"> <rdfs:label xml:lang="en">specifies royalty</rdfs:label> <rdfs:comment xml:lang="en">Links a contract (e.g., RecordingContract, PublishingContract) to the Royalty terms it defines. Passman Part II (Record Deals) and Part III (Publishing).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;Royalty"/> <owl:inverseOf rdf:resource="&arl;royaltySpecifiedIn"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;royaltySpecifiedIn"> <rdfs:label xml:lang="en">royalty specified in</rdfs:label> <rdfs:comment xml:lang="en">Links Royalty terms back to the Contract that defines them. Inverse of specifiesRoyalty.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Royalty"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;specifiesRoyalty"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;coversTerritory"> <rdfs:label xml:lang="en">covers territory</rdfs:label> <rdfs:comment xml:lang="en">Specifies the geographical Territory an Agreement or License applies to. Passman discusses territory for record deals (p. 161), publishing (subpublishing, p. 264), etc.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Agreement"/> <rdfs:range rdf:resource="&arl;Territory"/> <owl:inverseOf rdf:resource="&arl;territoryCoveredBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;territoryCoveredBy"> <rdfs:label xml:lang="en">territory covered by</rdfs:label> <rdfs:comment xml:lang="en">Indicates which Agreements or Licenses apply to a specific Territory. Inverse of coversTerritory.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Territory"/> <rdfs:range rdf:resource="&arl;Agreement"/> <owl:inverseOf rdf:resource="&arl;coversTerritory"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasDealTerm"> <rdfs:label xml:lang="en">has deal term</rdfs:label> <rdfs:comment xml:lang="en">Links a Contract to its duration or term structure (initial period, options). Passman p. 111 (record deals), p. 299 (songwriter deals).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;DealTerm"/> <owl:inverseOf rdf:resource="&arl;isTermOfDeal"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isTermOfDeal"> <rdfs:label xml:lang="en">is term of deal</rdfs:label> <rdfs:comment xml:lang="en">Links a DealTerm back to the Contract it belongs to. Inverse of hasDealTerm.</rdfs:comment> <rdfs:domain rdf:resource="&arl;DealTerm"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;hasDealTerm"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;optionFor"> <rdfs:label xml:lang="en">option for</rdfs:label> <rdfs:comment xml:lang="en">Indicates that a DealTerm represents an option for a specific deliverable, typically a MusicRelease or number of MusicWorks. Passman p. 107-110.</rdfs:comment> <rdfs:domain rdf:resource="&arl;DealTerm"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicRelease"/> <rdf:Description rdf:about="&arl;DeliveryRequirement"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;optionDefinedInTerm"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;optionDefinedInTerm"> <rdfs:label xml:lang="en">option defined in term</rdfs:label> <rdfs:comment xml:lang="en">Relates a deliverable (MusicRelease, DeliveryRequirement) back to the DealTerm that specifies it as an option. Inverse of optionFor.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicRelease"/> <rdf:Description rdf:about="&arl;DeliveryRequirement"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;DealTerm"/> <owl:inverseOf rdf:resource="&arl;optionFor"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;administersWork"> <rdfs:label xml:lang="en">administers work</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicPublisher or AdministrationAgreement to the MusicWork(s) being administered. Passman p. 187, p. 310.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicPublisher"/> <rdf:Description rdf:about="&arl;AdministrationAgreement"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;MusicWork"/> <owl:inverseOf rdf:resource="&arl;workAdministeredBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;workAdministeredBy"> <rdfs:label xml:lang="en">work administered by</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicWork to the MusicPublisher or AdministrationAgreement that administers it. Inverse of administersWork.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicWork"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicPublisher"/> <rdf:Description rdf:about="&arl;AdministrationAgreement"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;administersWork"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;grantsLicense"> <rdfs:label xml:lang="en">grants license</rdfs:label> <rdfs:comment xml:lang="en">Relates a licensor (EPMO) to the LicenseAgreement they are granting.</rdfs:comment> <rdfs:domain rdf:resource="&arl;EPMO"/> <rdfs:range rdf:resource="&arl;LicenseAgreement"/> <owl:inverseOf rdf:resource="&arl;licenseGrantedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;licenseGrantedBy"> <rdfs:label xml:lang="en">license granted by</rdfs:label> <rdfs:comment xml:lang="en">Relates a LicenseAgreement to the EPMO that granted it. Inverse of grantsLicense.</rdfs:comment> <rdfs:domain rdf:resource="&arl;LicenseAgreement"/> <rdfs:range rdf:resource="&arl;EPMO"/> <owl:inverseOf rdf:resource="&arl;grantsLicense"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;specifiesFund"> <rdfs:label xml:lang="en">specifies fund</rdfs:label> <rdfs:comment xml:lang="en">Links a RecordingContract to a Fund, which includes recording costs and artist advance. Passman p. 98-99.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RecordingContract"/> <rdfs:range rdf:resource="&arl;Fund"/> <owl:inverseOf rdf:resource="&arl;fundSpecifiedIn"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;fundSpecifiedIn"> <rdfs:label xml:lang="en">fund specified in</rdfs:label> <rdfs:comment xml:lang="en">Links a Fund back to the RecordingContract that specifies it. Inverse of specifiesFund.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Fund"/> <rdfs:range rdf:resource="&arl;RecordingContract"/> <owl:inverseOf rdf:resource="&arl;specifiesFund"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasWriter"> <rdfs:label xml:lang="en">has writer</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicWork to its Songwriter(s).</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicWork"/> <rdfs:range rdf:resource="&arl;Songwriter"/> <owl:inverseOf rdf:resource="&arl;isWriterOf"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isWriterOf"> <rdfs:label xml:lang="en">is writer of</rdfs:label> <rdfs:comment xml:lang="en">Relates a Songwriter to the MusicWork(s) they have written. Inverse of hasWriter.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Songwriter"/> <rdfs:range rdf:resource="&arl;MusicWork"/> <owl:inverseOf rdf:resource="&arl;hasWriter"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasArtist"> <rdfs:label xml:lang="en">has artist</rdfs:label> <rdfs:comment xml:lang="en">Relates a Recording to its performing Artist(s).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Recording"/> <rdfs:range rdf:resource="&arl;Artist"/> <owl:inverseOf rdf:resource="&arl;performedOnRecording"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;performedOnRecording"> <rdfs:label xml:lang="en">performed on recording</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist to the Recording(s) they performed on. Inverse of hasArtist.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;Recording"/> <owl:inverseOf rdf:resource="&arl;hasArtist"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isRecordingOf"> <rdfs:label xml:lang="en">is recording of</rdfs:label> <rdfs:comment xml:lang="en">Relates a Recording (master) to the MusicWork (song) it embodies.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Recording"/> <rdfs:range rdf:resource="&arl;MusicWork"/> <owl:inverseOf rdf:resource="&arl;hasRecording"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasRecording"> <rdfs:label xml:lang="en">has recording</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicWork to the Recording(s) that embody it. Inverse of isRecordingOf.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicWork"/> <rdfs:range rdf:resource="&arl;Recording"/> <owl:inverseOf rdf:resource="&arl;isRecordingOf"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;containsRecording"> <rdfs:label xml:lang="en">contains recording</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicRelease (e.g., album) to the Recordings it includes.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicRelease"/> <rdfs:range rdf:resource="&arl;Recording"/> <owl:inverseOf rdf:resource="&arl;recordingContainedInRelease"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;recordingContainedInRelease"> <rdfs:label xml:lang="en">recording contained in release</rdfs:label> <rdfs:comment xml:lang="en">Relates a Recording to the MusicRelease(s) it is part of. Inverse of containsRecording.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Recording"/> <rdfs:range rdf:resource="&arl;MusicRelease"/> <owl:inverseOf rdf:resource="&arl;containsRecording"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;signsContract"> <rdfs:label xml:lang="en">signs contract</rdfs:label> <rdfs:comment xml:lang="en">General property linking an EPMO to a Contract they are a party to.</rdfs:comment> <rdfs:domain rdf:resource="&arl;EPMO"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;contractSignedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;contractSignedBy"> <rdfs:label xml:lang="en">contract signed by</rdfs:label> <rdfs:comment xml:lang="en">Relates a Contract to the EPMO(s) who signed it. Inverse of signsContract.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;EPMO"/> <owl:inverseOf rdf:resource="&arl;signsContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;partiesInvolved"> <rdfs:label xml:lang="en">parties involved</rdfs:label> <rdfs:comment xml:lang="en">Specifies the EPMOs (Persons or Organizations) that are parties to an Agreement.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Agreement"/> <rdfs:range rdf:resource="&arl;EPMO"/> <owl:inverseOf rdf:resource="&arl;isPartyToAgreement"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isPartyToAgreement"> <rdfs:label xml:lang="en">is party to agreement</rdfs:label> <rdfs:comment xml:lang="en">Indicates an EPMO is a party to a specific Agreement. Inverse of partiesInvolved.</rdfs:comment> <rdfs:domain rdf:resource="&arl;EPMO"/> <rdfs:range rdf:resource="&arl;Agreement"/> <owl:inverseOf rdf:resource="&arl;partiesInvolved"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;specifiesAdvance"> <rdfs:label xml:lang="en">specifies advance</rdfs:label> <rdfs:comment xml:lang="en">Links a contract (e.g., RecordingContract, PublishingContract) to an Advance payment. Passman p. 77, p. 98, p. 301.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;Advance"/> <owl:inverseOf rdf:resource="&arl;advanceSpecifiedInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;advanceSpecifiedInContract"> <rdfs:label xml:lang="en">advance specified in contract</rdfs:label> <rdfs:comment xml:lang="en">Links an Advance to the Contract that specifies it. Inverse of specifiesAdvance.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Advance"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;specifiesAdvance"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasManager"> <rdfs:label xml:lang="en">has manager</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist to their PersonalManager. Consider using hasTeamMember with range PersonalManager for consistency.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;PersonalManager"/> <owl:inverseOf rdf:resource="&arl;managesArtist"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;managesArtist"> <rdfs:label xml:lang="en">manages artist</rdfs:label> <rdfs:comment xml:lang="en">Relates a PersonalManager to the Artist they manage. Inverse of hasManager.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PersonalManager"/> <rdfs:range rdf:resource="&arl;Artist"/> <owl:inverseOf rdf:resource="&arl;hasManager"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasControlledCompositionClause"> <rdfs:label xml:lang="en">has controlled composition clause</rdfs:label> <rdfs:comment xml:lang="en">Links a RecordingContract to its ControlledCompositionClause. Passman p. 275-283.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RecordingContract"/> <rdfs:range rdf:resource="&arl;ControlledCompositionClause"/> <owl:inverseOf rdf:resource="&arl;controlledCompositionClauseInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;controlledCompositionClauseInContract"> <rdfs:label xml:lang="en">controlled composition clause in contract</rdfs:label> <rdfs:comment xml:lang="en">Links a ControlledCompositionClause to the RecordingContract it is part of. Inverse of hasControlledCompositionClause.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ControlledCompositionClause"/> <rdfs:range rdf:resource="&arl;RecordingContract"/> <owl:inverseOf rdf:resource="&arl;hasControlledCompositionClause"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;artistHasGenre"> <rdfs:label xml:lang="en">artist has genre</rdfs:label> <rdfs:comment xml:lang="en">Associates an Artist with one or more musical Genres.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;Genre"/> <owl:inverseOf rdf:resource="&arl;genreOfArtist"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;genreOfArtist"> <rdfs:label xml:lang="en">genre of artist</rdfs:label> <rdfs:comment xml:lang="en">Associates a Genre with Artists who perform in that genre. Inverse of artistHasGenre.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Genre"/> <rdfs:range rdf:resource="&arl;Artist"/> <owl:inverseOf rdf:resource="&arl;artistHasGenre"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasPayOrPlayClause"> <rdfs:label xml:lang="en">has pay or play clause</rdfs:label> <rdfs:comment xml:lang="en">Links a contract (typically RecordingContract or film composer agreement) to a PayOrPlayProvision. Passman p. 110.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;PayOrPlayProvision"/> <owl:inverseOf rdf:resource="&arl;payOrPlayClauseInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;payOrPlayClauseInContract"> <rdfs:label xml:lang="en">pay or play clause in contract</rdfs:label> <rdfs:comment xml:lang="en">Links a PayOrPlayProvision to the Contract it is part of. Inverse of hasPayOrPlayClause.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PayOrPlayProvision"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;hasPayOrPlayClause"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasThreeSixtyProvision"> <rdfs:label xml:lang="en">has 360 provision</rdfs:label> <rdfs:comment xml:lang="en">Links a RecordingContract to a ThreeSixtyRightProvision where the label participates in non-record income. Passman p. 91-93.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RecordingContract"/> <rdfs:range rdf:resource="&arl;ThreeSixtyRightProvision"/> <owl:inverseOf rdf:resource="&arl;threeSixtyProvisionInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;threeSixtyProvisionInContract"> <rdfs:label xml:lang="en">360 provision in contract</rdfs:label> <rdfs:comment xml:lang="en">Links a ThreeSixtyRightProvision to the RecordingContract it is part of. Inverse of hasThreeSixtyProvision.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ThreeSixtyRightProvision"/> <rdfs:range rdf:resource="&arl;RecordingContract"/> <owl:inverseOf rdf:resource="&arl;hasThreeSixtyProvision"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;grantsParticipationIn"> <rdfs:label xml:lang="en">grants participation in</rdfs:label> <rdfs:comment xml:lang="en">Specifies the AncillaryIncomeStream (e.g., touring, merch) a ThreeSixtyRightProvision allows the label to participate in.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ThreeSixtyRightProvision"/> <rdfs:range rdf:resource="&arl;AncillaryIncomeStream"/> <owl:inverseOf rdf:resource="&arl;participationGrantedByProvision"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;participationGrantedByProvision"> <rdfs:label xml:lang="en">participation granted by provision</rdfs:label> <rdfs:comment xml:lang="en">Relates an AncillaryIncomeStream to the ThreeSixtyRightProvision that grants participation in it. Inverse of grantsParticipationIn.</rdfs:comment> <rdfs:domain rdf:resource="&arl;AncillaryIncomeStream"/> <rdfs:range rdf:resource="&arl;ThreeSixtyRightProvision"/> <owl:inverseOf rdf:resource="&arl;grantsParticipationIn"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasReversionClause"> <rdfs:label xml:lang="en">has reversion clause</rdfs:label> <rdfs:comment xml:lang="en">Links a PublishingContract (or SongwriterAgreement) to a ReversionClause detailing how/when copyrights revert to the songwriter. Passman p. 307-309.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PublishingAgreement"/> <rdfs:range rdf:resource="&arl;ReversionClause"/> <owl:inverseOf rdf:resource="&arl;reversionClauseInAgreement"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;reversionClauseInAgreement"> <rdfs:label xml:lang="en">reversion clause in agreement</rdfs:label> <rdfs:comment xml:lang="en">Links a ReversionClause to the PublishingAgreement it is part of. Inverse of hasReversionClause.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ReversionClause"/> <rdfs:range rdf:resource="&arl;PublishingAgreement"/> <owl:inverseOf rdf:resource="&arl;hasReversionClause"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;licensedForMediaType"> <rdfs:label xml:lang="en">licensed for media type</rdfs:label> <rdfs:comment xml:lang="en">Specifies the MediaType (e.g., Film, Television, Commercial) a SynchronizationLicense applies to. Passman p. 252.</rdfs:comment> <rdfs:domain rdf:resource="&arl;SynchronizationLicense"/> <rdfs:range rdf:resource="&arl;MediaType"/> <owl:inverseOf rdf:resource="&arl;mediaTypeLicensedVia"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;mediaTypeLicensedVia"> <rdfs:label xml:lang="en">media type licensed via</rdfs:label> <rdfs:comment xml:lang="en">Indicates a MediaType is subject to a SynchronizationLicense. Inverse of licensedForMediaType.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MediaType"/> <rdfs:range rdf:resource="&arl;SynchronizationLicense"/> <owl:inverseOf rdf:resource="&arl;licensedForMediaType"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;copyrightRegisteredFor"> <rdfs:label xml:lang="en">copyright registered for</rdfs:label> <rdfs:comment xml:lang="en">Links a CopyrightRegistration to the CreativeWork it pertains to. Passman p. 354.</rdfs:comment> <rdfs:domain rdf:resource="&arl;CopyrightRegistration"/> <rdfs:range rdf:resource="&arl;CreativeWork"/> <owl:inverseOf rdf:resource="&arl;creativeWorkHasRegistration"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;creativeWorkHasRegistration"> <rdfs:label xml:lang="en">creative work has registration</rdfs:label> <rdfs:comment xml:lang="en">Links a CreativeWork to its CopyrightRegistration. Inverse of copyrightRegisteredFor.</rdfs:comment> <rdfs:domain rdf:resource="&arl;CreativeWork"/> <rdfs:range rdf:resource="&arl;CopyrightRegistration"/> <owl:inverseOf rdf:resource="&arl;copyrightRegisteredFor"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;copyrightOwner"> <rdfs:label xml:lang="en">copyright owner</rdfs:label> <rdfs:comment xml:lang="en">Identifies the EPMO (Person or Organization) that owns a Copyright. Passman p. 215, p. 317.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Copyright"/> <rdfs:range rdf:resource="&arl;EPMO"/> <owl:inverseOf rdf:resource="&arl;ownsCopyright"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;ownsCopyright"> <rdfs:label xml:lang="en">owns copyright</rdfs:label> <rdfs:comment xml:lang="en">Relates an EPMO to a Copyright they own. Inverse of copyrightOwner.</rdfs:comment> <rdfs:domain rdf:resource="&arl;EPMO"/> <rdfs:range rdf:resource="&arl;Copyright"/> <owl:inverseOf rdf:resource="&arl;copyrightOwner"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasInternalAgreement"> <rdfs:label xml:lang="en">has internal agreement</rdfs:label> <rdfs:comment xml:lang="en">Links a MusicGroup to their MusicGroupInternalAgreement. Passman p. 365.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicGroup"/> <rdfs:range rdf:resource="&arl;MusicGroupInternalAgreement"/> <owl:inverseOf rdf:resource="&arl;internalAgreementForGroup"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;internalAgreementForGroup"> <rdfs:label xml:lang="en">internal agreement for group</rdfs:label> <rdfs:comment xml:lang="en">Links a MusicGroupInternalAgreement to the MusicGroup it governs. Inverse of hasInternalAgreement.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicGroupInternalAgreement"/> <rdfs:range rdf:resource="&arl;MusicGroup"/> <owl:inverseOf rdf:resource="&arl;hasInternalAgreement"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;agreementIncludesProvision"> <rdfs:label xml:lang="en">agreement includes provision</rdfs:label> <rdfs:comment xml:lang="en">A general property linking an Agreement to a specific ContractualProvision it contains.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Agreement"/> <rdfs:range rdf:resource="&arl;ContractualProvision"/> <owl:inverseOf rdf:resource="&arl;provisionIncludedInAgreement"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;provisionIncludedInAgreement"> <rdfs:label xml:lang="en">provision included in agreement</rdfs:label> <rdfs:comment xml:lang="en">Relates a ContractualProvision to the Agreement it is part of. Inverse of agreementIncludesProvision.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ContractualProvision"/> <rdfs:range rdf:resource="&arl;Agreement"/> <owl:inverseOf rdf:resource="&arl;agreementIncludesProvision"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;provisionAppliesTo"> <rdfs:label xml:lang="en">provision applies to</rdfs:label> <rdfs:comment xml:lang="en">Links a ContractualProvision to the entity or concept it governs (e.g., a MusicWork for Controlled Compositions, an Artist for Key Member).</rdfs:comment> <rdfs:domain rdf:resource="&arl;ContractualProvision"/> <rdfs:range rdf:resource="&owl;Thing"/> <owl:inverseOf rdf:resource="&arl;isGovernedByProvision"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isGovernedByProvision"> <rdfs:label xml:lang="en">is governed by provision</rdfs:label> <rdfs:comment xml:lang="en">Indicates an entity is subject to a specific ContractualProvision. Inverse of provisionAppliesTo.</rdfs:comment> <rdfs:domain rdf:resource="&owl;Thing"/> <rdfs:range rdf:resource="&arl;ContractualProvision"/> <owl:inverseOf rdf:resource="&arl;provisionAppliesTo"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;organizedBy"> <rdfs:label xml:lang="en">organized by</rdfs:label> <rdfs:comment xml:lang="en">Relates an Event (e.g., Tour) to the EPMO (e.g., Promoter, PersonalManager) responsible for its organization. Passman p. 395.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Event"/> <rdfs:range rdf:resource="&arl;EPMO"/> <owl:inverseOf rdf:resource="&arl;organizesEvent"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;organizesEvent"> <rdfs:label xml:lang="en">organizes event</rdfs:label> <rdfs:comment xml:lang="en">Relates an EPMO to an Event they organize. Inverse of organizedBy.</rdfs:comment> <rdfs:domain rdf:resource="&arl;EPMO"/> <rdfs:range rdf:resource="&arl;Event"/> <owl:inverseOf rdf:resource="&arl;organizedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;takesPlaceAt"> <rdfs:label xml:lang="en">takes place at</rdfs:label> <rdfs:comment xml:lang="en">Links an Event (e.g., Concert) to the Venue where it occurs.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Event"/> <rdfs:range rdf:resource="&arl;Venue"/> <owl:inverseOf rdf:resource="&arl;hostsEvent"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hostsEvent"> <rdfs:label xml:lang="en">hosts event</rdfs:label> <rdfs:comment xml:lang="en">Relates a Venue to an Event it hosts. Inverse of takesPlaceAt.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Venue"/> <rdfs:range rdf:resource="&arl;Event"/> <owl:inverseOf rdf:resource="&arl;takesPlaceAt"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;eventFeaturesArtist"> <rdfs:label xml:lang="en">event features artist</rdfs:label> <rdfs:comment xml:lang="en">Relates an Event to the Artist(s) performing at it.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Event"/> <rdfs:range rdf:resource="&arl;Artist"/> <owl:inverseOf rdf:resource="&arl;artistParticipatesInEvent"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;artistParticipatesInEvent"> <rdfs:label xml:lang="en">artist participates in event</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist to an Event they are participating in. Inverse of eventFeaturesArtist.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;Event"/> <owl:inverseOf rdf:resource="&arl;eventFeaturesArtist"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;tourConsistsOf"> <rdfs:label xml:lang="en">tour consists of</rdfs:label> <rdfs:comment xml:lang="en">Relates a Tour to the individual Concerts that make up the tour.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Tour"/> <rdfs:range rdf:resource="&arl;Concert"/> <owl:inverseOf rdf:resource="&arl;concertPartOfTour"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;concertPartOfTour"> <rdfs:label xml:lang="en">concert part of tour</rdfs:label> <rdfs:comment xml:lang="en">Relates a Concert to the Tour it is part of. Inverse of tourConsistsOf.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Concert"/> <rdfs:range rdf:resource="&arl;Tour"/> <owl:inverseOf rdf:resource="&arl;tourConsistsOf"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;sellsTicket"> <rdfs:label xml:lang="en">sells ticket</rdfs:label> <rdfs:comment xml:lang="en">Relates an entity (Promoter, Venue, TicketingAgent) to a Ticket they sell.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Promoter"/> <rdf:Description rdf:about="&arl;Venue"/> <rdf:Description rdf:about="&arl;TicketingAgent"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;Ticket"/> <owl:inverseOf rdf:resource="&arl;ticketSoldBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;ticketSoldBy"> <rdfs:label xml:lang="en">ticket sold by</rdfs:label> <rdfs:comment xml:lang="en">Relates a Ticket to the entity that sold it. Inverse of sellsTicket.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Ticket"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Promoter"/> <rdf:Description rdf:about="&arl;Venue"/> <rdf:Description rdf:about="&arl;TicketingAgent"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;sellsTicket"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;ticketForEvent"> <rdfs:label xml:lang="en">ticket for event</rdfs:label> <rdfs:comment xml:lang="en">Links a Ticket to the specific Event it grants access to.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Ticket"/> <rdfs:range rdf:resource="&arl;Event"/> <owl:inverseOf rdf:resource="&arl;eventHasTicket"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;eventHasTicket"> <rdfs:label xml:lang="en">event has ticket</rdfs:label> <rdfs:comment xml:lang="en">Relates an Event to the Tickets sold for it. Inverse of ticketForEvent.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Event"/> <rdfs:range rdf:resource="&arl;Ticket"/> <owl:inverseOf rdf:resource="&arl;ticketForEvent"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;licensesMerchandiseRightsTo"> <rdfs:label xml:lang="en">licenses merchandise rights to</rdfs:label> <rdfs:comment xml:lang="en">Relates an Artist (or their rights-holding entity) to a Merchandiser. Passman Ch. 24.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Artist"/> <rdfs:range rdf:resource="&arl;Merchandiser"/> <owl:inverseOf rdf:resource="&arl;merchandiseRightsLicensedFrom"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;merchandiseRightsLicensedFrom"> <rdfs:label xml:lang="en">merchandise rights licensed from</rdfs:label> <rdfs:comment xml:lang="en">Relates a Merchandiser to the Artist from whom they license rights. Inverse of licensesMerchandiseRightsTo.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Merchandiser"/> <rdfs:range rdf:resource="&arl;Artist"/> <owl:inverseOf rdf:resource="&arl;licensesMerchandiseRightsTo"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;sellsMerchandiseItem"> <rdfs:label xml:lang="en">sells merchandise item</rdfs:label> <rdfs:comment xml:lang="en">Relates a Merchandiser or Venue to a MerchandiseItem they sell.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Merchandiser"/> <rdf:Description rdf:about="&arl;Venue"/> <rdf:Description rdf:about="&arl;Artist"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;MerchandiseItem"/> <owl:inverseOf rdf:resource="&arl;merchandiseItemSoldBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;merchandiseItemSoldBy"> <rdfs:label xml:lang="en">merchandise item sold by</rdfs:label> <rdfs:comment xml:lang="en">Relates a MerchandiseItem to the entity that sells it. Inverse of sellsMerchandiseItem.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MerchandiseItem"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Merchandiser"/> <rdf:Description rdf:about="&arl;Venue"/> <rdf:Description rdf:about="&arl;Artist"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;sellsMerchandiseItem"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;royaltyAppliesTo"> <rdfs:label xml:lang="en">royalty applies to</rdfs:label> <rdfs:comment xml:lang="en">Specifies what a Royalty is paid on (e.g., a MusicRelease, a MusicWork, a MerchandiseItem).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Royalty"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicRelease"/> <rdf:Description rdf:about="&arl;MusicWork"/> <rdf:Description rdf:about="&arl;MerchandiseItem"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;subjectOfRoyalty"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;subjectOfRoyalty"> <rdfs:label xml:lang="en">subject of royalty</rdfs:label> <rdfs:comment xml:lang="en">Indicates a CreativeWork, MerchandiseItem or RevenueSource is subject to a Royalty. Inverse of royaltyAppliesTo.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;MusicRelease"/> <rdf:Description rdf:about="&arl;MusicWork"/> <rdf:Description rdf:about="&arl;MerchandiseItem"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;Royalty"/> <owl:inverseOf rdf:resource="&arl;royaltyAppliesTo"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;advanceIsRecoupableFrom"> <rdfs:label xml:lang="en">advance is recoupable from</rdfs:label> <rdfs:comment xml:lang="en">Specifies the Royalty stream(s) or RevenueSource from which an Advance is recouped. Passman p. 77.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Advance"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Royalty"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;recoupsAdvance"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;recoupsAdvance"> <rdfs:label xml:lang="en">recoups advance</rdfs:label> <rdfs:comment xml:lang="en">Indicates a Royalty or RevenueSource is used to recoup an Advance. Inverse of advanceIsRecoupableFrom.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Royalty"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;Advance"/> <owl:inverseOf rdf:resource="&arl;advanceIsRecoupableFrom"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;costIsRecoupableFrom"> <rdfs:label xml:lang="en">cost is recoupable from</rdfs:label> <rdfs:comment xml:lang="en">Specifies the Royalty stream(s) or RevenueSource from which a RecoupableCost is recouped.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RecoupableCost"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Royalty"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;recoupsCost"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;recoupsCost"> <rdfs:label xml:lang="en">recoups cost</rdfs:label> <rdfs:comment xml:lang="en">Indicates a Royalty or RevenueSource is used to recoup a RecoupableCost. Inverse of costIsRecoupableFrom.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Royalty"/> <rdf:Description rdf:about="&arl;RevenueSource"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;RecoupableCost"/> <owl:inverseOf rdf:resource="&arl;costIsRecoupableFrom"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;licenseHasClause"> <rdfs:label xml:lang="en">license has clause</rdfs:label> <rdfs:comment xml:lang="en">Links a LicenseAgreement to a specific LicenseClause within it.</rdfs:comment> <rdfs:domain rdf:resource="&arl;LicenseAgreement"/> <rdfs:range rdf:resource="&arl;LicenseClause"/> <owl:inverseOf rdf:resource="&arl;clauseInLicense"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;clauseInLicense"> <rdfs:label xml:lang="en">clause in license</rdfs:label> <rdfs:comment xml:lang="en">Relates a LicenseClause to the LicenseAgreement it is part of. Inverse of licenseHasClause.</rdfs:comment> <rdfs:domain rdf:resource="&arl;LicenseClause"/> <rdfs:range rdf:resource="&arl;LicenseAgreement"/> <owl:inverseOf rdf:resource="&arl;licenseHasClause"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;publishingIncomeFromSource"> <rdfs:label xml:lang="en">publishing income from source</rdfs:label> <rdfs:comment xml:lang="en">Relates a MusicPublisher to the IncomeSourceType of their publishing revenue (e.g., Mechanical, Performance, Sync). Passman p. 190.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicPublisher"/> <rdfs:range rdf:resource="&arl;IncomeSourceType"/> <owl:inverseOf rdf:resource="&arl;sourceForPublishingIncome"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;sourceForPublishingIncome"> <rdfs:label xml:lang="en">source for publishing income</rdfs:label> <rdfs:comment xml:lang="en">Relates an IncomeSourceType to MusicPublishers who derive income from it. Inverse of publishingIncomeFromSource.</rdfs:comment> <rdfs:domain rdf:resource="&arl;IncomeSourceType"/> <rdfs:range rdf:resource="&arl;MusicPublisher"/> <owl:inverseOf rdf:resource="&arl;publishingIncomeFromSource"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;contractSpecifiesDeliveryRequirement"> <rdfs:label xml:lang="en">contract specifies delivery requirement</rdfs:label> <rdfs:comment xml:lang="en">Links a Contract (e.g., RecordingContract) to the DeliveryRequirements for creative works. Passman p. 115.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;DeliveryRequirement"/> <owl:inverseOf rdf:resource="&arl;deliveryRequirementSpecifiedInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;deliveryRequirementSpecifiedInContract"> <rdfs:label xml:lang="en">delivery requirement specified in contract</rdfs:label> <rdfs:comment xml:lang="en">Relates a DeliveryRequirement to the Contract that specifies it. Inverse of contractSpecifiesDeliveryRequirement.</rdfs:comment> <rdfs:domain rdf:resource="&arl;DeliveryRequirement"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;contractSpecifiesDeliveryRequirement"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;clauseAppliesToMusicWork"> <rdfs:label xml:lang="en">clause applies to music work</rdfs:label> <rdfs:comment xml:lang="en">Specifically links a ControlledCompositionClause to the MusicWork(s) it governs.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ControlledCompositionClause"/> <rdfs:range rdf:resource="&arl;MusicWork"/> <owl:inverseOf rdf:resource="&arl;musicWorkHasControlledCompositionTerms"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;musicWorkHasControlledCompositionTerms"> <rdfs:label xml:lang="en">music work has controlled composition terms</rdfs:label> <rdfs:comment xml:lang="en">Links a MusicWork to the ControlledCompositionClause that applies to it. Inverse of clauseAppliesToMusicWork.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicWork"/> <rdfs:range rdf:resource="&arl;ControlledCompositionClause"/> <owl:inverseOf rdf:resource="&arl;clauseAppliesToMusicWork"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;escalationTriggeredBy"> <rdfs:label xml:lang="en">escalation triggered by</rdfs:label> <rdfs:comment xml:lang="en">Links a RoyaltyTier to the PerformanceMetricThreshold that triggers the escalated rate. Passman p. 97.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RoyaltyTier"/> <rdfs:range rdf:resource="&arl;PerformanceMetricThreshold"/> <owl:inverseOf rdf:resource="&arl;triggersRoyaltyTierEscalation"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;triggersRoyaltyTierEscalation"> <rdfs:label xml:lang="en">triggers royalty tier escalation</rdfs:label> <rdfs:comment xml:lang="en">Relates a PerformanceMetricThreshold to the RoyaltyTier whose escalation it triggers. Inverse of escalationTriggeredBy.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PerformanceMetricThreshold"/> <rdfs:range rdf:resource="&arl;RoyaltyTier"/> <owl:inverseOf rdf:resource="&arl;escalationTriggeredBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;optionExercisableBy"> <rdfs:label xml:lang="en">option exercisable by</rdfs:label> <rdfs:comment xml:lang="en">Indicates which party (typically RecordLabel or MusicPublisher) has the right to exercise an option in a DealTerm.</rdfs:comment> <rdfs:domain rdf:resource="&arl;DealTerm"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;RecordLabel"/> <rdf:Description rdf:about="&arl;MusicPublisher"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;canExerciseOptionInTerm"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;canExerciseOptionInTerm"> <rdfs:label xml:lang="en">can exercise option in term</rdfs:label> <rdfs:comment xml:lang="en">Indicates a RecordLabel or MusicPublisher can exercise an option defined in a DealTerm. Inverse of optionExercisableBy.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;RecordLabel"/> <rdf:Description rdf:about="&arl;MusicPublisher"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;DealTerm"/> <owl:inverseOf rdf:resource="&arl;optionExercisableBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;licenseGrantsUsageRight"> <rdfs:label xml:lang="en">license grants usage right</rdfs:label> <rdfs:comment xml:lang="en">Specifies the particular UsageRight (e.g., reproduce, distribute, publicly perform) granted by a LicenseAgreement.</rdfs:comment> <rdfs:domain rdf:resource="&arl;LicenseAgreement"/> <rdfs:range rdf:resource="&arl;UsageRight"/> <owl:inverseOf rdf:resource="&arl;usageRightGrantedByLicense"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;usageRightGrantedByLicense"> <rdfs:label xml:lang="en">usage right granted by license</rdfs:label> <rdfs:comment xml:lang="en">Relates a UsageRight to the LicenseAgreement that grants it. Inverse of licenseGrantsUsageRight.</rdfs:comment> <rdfs:domain rdf:resource="&arl;UsageRight"/> <rdfs:range rdf:resource="&arl;LicenseAgreement"/> <owl:inverseOf rdf:resource="&arl;licenseGrantsUsageRight"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;copyrightAppliesToTerritory"> <rdfs:label xml:lang="en">copyright applies to territory</rdfs:label> <rdfs:comment xml:lang="en">Specifies the Territory where a Copyright is valid.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Copyright"/> <rdfs:range rdf:resource="&arl;Territory"/> <owl:inverseOf rdf:resource="&arl;territoryOfCopyright"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;territoryOfCopyright"> <rdfs:label xml:lang="en">territory of copyright</rdfs:label> <rdfs:comment xml:lang="en">Indicates a Territory is covered by a Copyright. Inverse of copyrightAppliesToTerritory.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Territory"/> <rdfs:range rdf:resource="&arl;Copyright"/> <owl:inverseOf rdf:resource="&arl;copyrightAppliesToTerritory"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;royaltyHasTier"> <rdfs:label xml:lang="en">royalty has tier</rdfs:label> <rdfs:comment xml:lang="en">Links a Royalty to its various RoyaltyTiers (for escalations).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Royalty"/> <rdfs:range rdf:resource="&arl;RoyaltyTier"/> <owl:inverseOf rdf:resource="&arl;tierOfRoyalty"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;tierOfRoyalty"> <rdfs:label xml:lang="en">tier of royalty</rdfs:label> <rdfs:comment xml:lang="en">Relates a RoyaltyTier to the Royalty it belongs to. Inverse of royaltyHasTier.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RoyaltyTier"/> <rdfs:range rdf:resource="&arl;Royalty"/> <owl:inverseOf rdf:resource="&arl;royaltyHasTier"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isCrossCollateralizedWith"> <rdfs:label xml:lang="en">is cross-collateralized with</rdfs:label> <rdfs:comment xml:lang="en">Indicates that recoupment for one financial item (e.g., an Advance or Contract's deficit) can be made from another. Passman p. 89-91. This property is symmetric.</rdfs:comment> <rdf:type rdf:resource="&owl;SymmetricProperty"/> <rdfs:domain rdf:resource="&arl;FinancialConcept"/> <rdfs:range rdf:resource="&arl;FinancialConcept"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;contractIncludesExclusivity"> <rdfs:label xml:lang="en">contract includes exclusivity</rdfs:label> <rdfs:comment xml:lang="en">Links a Contract to its ExclusivityProvision. Passman p. 174 (record deals).</rdfs:comment> <rdfs:domain rdf:resource="&arl;Contract"/> <rdfs:range rdf:resource="&arl;ExclusivityProvision"/> <owl:inverseOf rdf:resource="&arl;exclusivityProvisionInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;exclusivityProvisionInContract"> <rdfs:label xml:lang="en">exclusivity provision in contract</rdfs:label> <rdfs:comment xml:lang="en">Relates an ExclusivityProvision to the Contract it is part of. Inverse of contractIncludesExclusivity.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ExclusivityProvision"/> <rdfs:range rdf:resource="&arl;Contract"/> <owl:inverseOf rdf:resource="&arl;contractIncludesExclusivity"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;contractIncludesReRecordingRestriction"> <rdfs:label xml:lang="en">contract includes re-recording restriction</rdfs:label> <rdfs:comment xml:lang="en">Links a RecordingContract to its ReRecordingRestrictionClause. Passman p. 178.</rdfs:comment> <rdfs:domain rdf:resource="&arl;RecordingContract"/> <rdfs:range rdf:resource="&arl;ReRecordingRestrictionClause"/> <owl:inverseOf rdf:resource="&arl;reRecordingRestrictionInContract"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;reRecordingRestrictionInContract"> <rdfs:label xml:lang="en">re-recording restriction in contract</rdfs:label> <rdfs:comment xml:lang="en">Relates a ReRecordingRestrictionClause to the RecordingContract it is part of. Inverse of contractIncludesReRecordingRestriction.</rdfs:comment> <rdfs:domain rdf:resource="&arl;ReRecordingRestrictionClause"/> <rdfs:range rdf:resource="&arl;RecordingContract"/> <owl:inverseOf rdf:resource="&arl;contractIncludesReRecordingRestriction"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasTerminationRightProvision"> <rdfs:label xml:lang="en">has termination right provision</rdfs:label> <rdfs:comment xml:lang="en">Indicates an Agreement or a Copyright grant is subject to a TerminationRightProvision. Passman p. 323, p. 348.</rdfs:comment> <rdfs:domain> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Agreement"/> <rdf:Description rdf:about="&arl;Copyright"/> </owl:unionOf> </owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;TerminationRightProvision"/> <owl:inverseOf rdf:resource="&arl;terminationRightFor"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;terminationRightFor"> <rdfs:label xml:lang="en">termination right for</rdfs:label> <rdfs:comment xml:lang="en">Relates a TerminationRightProvision to the Agreement or Copyright grant it applies to. Inverse of hasTerminationRightProvision.</rdfs:comment> <rdfs:domain rdf:resource="&arl;TerminationRightProvision"/> <rdfs:range> <owl:Class> <owl:unionOf rdf:parseType="Collection"> <rdf:Description rdf:about="&arl;Agreement"/> <rdf:Description rdf:about="&arl;Copyright"/> </owl:unionOf> </owl:Class> </rdfs:range> <owl:inverseOf rdf:resource="&arl;hasTerminationRightProvision"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;actionPerformedBy"> <rdfs:domain rdf:resource="&arl;AgentAction"/> <rdfs:range rdf:resource="&arl;AIAgent"/> <rdfs:label xml:lang="en">action performed by</rdfs:label> <owl:inverseOf rdf:resource="&arl;performsAction"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;performsAction"> <rdfs:label xml:lang="en">performs action</rdfs:label> <rdfs:comment xml:lang="en">Relates an AI Agent to the AgentActions it performs. Inverse of actionPerformedBy.</rdfs:comment> <rdfs:domain rdf:resource="&arl;AIAgent"/> <rdfs:range rdf:resource="&arl;AgentAction"/> <owl:inverseOf rdf:resource="&arl;actionPerformedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;actionTargetsEntity"> <rdfs:domain rdf:resource="&arl;AgentAction"/> <rdfs:range rdf:resource="&owl;Thing"/> <rdfs:label xml:lang="en">action targets entity</rdfs:label> <owl:inverseOf rdf:resource="&arl;isTargetOfAction"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;isTargetOfAction"> <rdfs:label xml:lang="en">is target of action</rdfs:label> <rdfs:comment xml:lang="en">Indicates an entity is the target of an AgentAction. Inverse of actionTargetsEntity.</rdfs:comment> <rdfs:domain rdf:resource="&owl;Thing"/> <rdfs:range rdf:resource="&arl;AgentAction"/> <owl:inverseOf rdf:resource="&arl;actionTargetsEntity"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;feedConsumedBy"> <rdfs:domain rdf:resource="&arl;ContextualDataFeed"/> <rdfs:range rdf:resource="&arl;AIAgent"/> <rdfs:label xml:lang="en">feed consumed by</rdfs:label> <owl:inverseOf rdf:resource="&arl;consumesFeed"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;consumesFeed"> <rdfs:label xml:lang="en">consumes feed</rdfs:label> <rdfs:comment xml:lang="en">Relates an AI Agent to the ContextualDataFeeds it consumes. Inverse of feedConsumedBy.</rdfs:comment> <rdfs:domain rdf:resource="&arl;AIAgent"/> <rdfs:range rdf:resource="&arl;ContextualDataFeed"/> <owl:inverseOf rdf:resource="&arl;feedConsumedBy"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;feedProvidesDataAbout"> <rdfs:domain rdf:resource="&arl;ContextualDataFeed"/> <rdfs:range rdf:resource="&owl;Class"/> <rdfs:label xml:lang="en">feed provides data about</rdfs:label> </owl:ObjectProperty> 
    
    <!-- Corrected Marketing Campaign Properties -->
    <owl:ObjectProperty rdf:about="&arl;campaignTargetsGenericAsset">
        <rdfs:label xml:lang="en">campaign targets generic asset</rdfs:label>
        <rdfs:comment xml:lang="en">A general property indicating a marketing campaign targets some asset (Artist or MusicRelease).</rdfs:comment>
        <rdfs:domain rdf:resource="&arl;MarketingCampaign"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&arl;MusicRelease"/>
                    <rdf:Description rdf:about="&arl;Artist"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <owl:inverseOf rdf:resource="&arl;isTargetOfMarketingCampaign"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&arl;targetsMusicRelease">
        <rdfs:subPropertyOf rdf:resource="&arl;campaignTargetsGenericAsset"/>
        <rdfs:label xml:lang="en">targets music release</rdfs:label>
        <rdfs:domain rdf:resource="&arl;MarketingCampaign"/>
        <rdfs:range rdf:resource="&arl;MusicRelease"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&arl;targetsArtist">
        <rdfs:subPropertyOf rdf:resource="&arl;campaignTargetsGenericAsset"/>
        <rdfs:label xml:lang="en">targets artist</rdfs:label>
        <rdfs:domain rdf:resource="&arl;MarketingCampaign"/>
        <rdfs:range rdf:resource="&arl;Artist"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="&arl;isTargetOfMarketingCampaign">
        <rdfs:label xml:lang="en">is target of marketing campaign</rdfs:label>
        <rdfs:comment xml:lang="en">Relates a MusicRelease or Artist to a MarketingCampaign targeting it. Inverse of campaignTargetsGenericAsset.</rdfs:comment>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&arl;MusicRelease"/>
                    <rdf:Description rdf:about="&arl;Artist"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="&arl;MarketingCampaign"/>
        <owl:inverseOf rdf:resource="&arl;campaignTargetsGenericAsset"/>
    </owl:ObjectProperty>
    <!-- End Corrected Marketing Campaign Properties -->

    <owl:ObjectProperty rdf:about="&arl;comprisesActivity"><rdfs:label xml:lang="en">comprises activity</rdfs:label><rdfs:domain rdf:resource="&arl;MarketingCampaign"/><rdfs:range rdf:resource="&arl;PromotionalActivity"/> <owl:inverseOf rdf:resource="&arl;activityPartOfCampaign"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;activityPartOfCampaign"> <rdfs:label xml:lang="en">activity part of campaign</rdfs:label> <rdfs:comment xml:lang="en">Relates a PromotionalActivity to the MarketingCampaign it belongs to. Inverse of comprisesActivity.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PromotionalActivity"/> <rdfs:range rdf:resource="&arl;MarketingCampaign"/> <owl:inverseOf rdf:resource="&arl;comprisesActivity"/> </owl:ObjectProperty>
