    <owl:ObjectProperty rdf:about="&arl;hasFundObject"> <rdfs:label xml:lang="en">has fund object</rdfs:label> <rdfs:comment xml:lang="en">Links a Fund to a MonetaryAmount representing the fund amount.</rdfs:comment> <rdfs:domain rdf:resource="&arl;Fund"/> <rdfs:range rdf:resource="&arl;MonetaryAmount"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasRecordingCostsObject"> <rdfs:label xml:lang="en">has recording costs object</rdfs:label> <rdfs:comment xml:lang="en">Links a Recording or RecoupableCost to a MonetaryAmount representing recording costs.</rdfs:comment> <rdfs:domain> <owl:Class><owl:unionOf rdf:parseType="Collection"><rdf:Description rdf:about="&arl;Recording"/><rdf:Description rdf:about="&arl;RecoupableCost"/></owl:unionOf></owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;MonetaryAmount"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasPPDObject"> <rdfs:label xml:lang="en">has PPD object</rdfs:label> <rdfs:comment xml:lang="en">Links a MusicRelease to a MonetaryAmount representing its PPD.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicRelease"/> <rdfs:range rdf:resource="&arl;MonetaryAmount"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasRecoupmentStatusObject"> <rdfs:label xml:lang="en">has recoupment status object</rdfs:label> <rdfs:comment xml:lang="en">Links an Artist or Contract to a MonetaryAmount representing their unrecouped balance.</rdfs:comment> <rdfs:domain> <owl:Class><owl:unionOf rdf:parseType="Collection"><rdf:Description rdf:about="&arl;Artist"/><rdf:Description rdf:about="&arl;Contract"/></owl:unionOf></owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&arl;MonetaryAmount"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;hasPayOrPlayPaymentObject"> <rdfs:label xml:lang="en">has pay or play payment object</rdfs:label> <rdfs:comment xml:lang="en">Links a PayOrPlayProvision to a MonetaryAmount representing the payment.</rdfs:comment> <rdfs:domain rdf:resource="&arl;PayOrPlayProvision"/> <rdfs:range rdf:resource="&arl;MonetaryAmount"/> </owl:ObjectProperty>
