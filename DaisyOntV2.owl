    <owl:DatatypeProperty rdf:about="&arl;statutoryRateApplies"> <rdfs:label xml:lang="en">statutory rate applies</rdfs:label> <rdfs:comment xml:lang="en">Boolean indicating if the statutory mechanical rate applies. Passman p.221.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MechanicalLicense"/> <rdfs:range rdf:resource="&xsd;boolean"/> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;streamingCount"> <rdfs:label xml:lang="en">streaming count</rdfs:label> <rdfs:comment xml:lang="en">Number of streams for a Recording or MusicRelease over a period.</rdfs:comment> <rdfs:domain> <owl:Class><owl:unionOf rdf:parseType="Collection"><rdf:Description rdf:about="&arl;Recording"/><rdf:Description rdf:about="&arl;MusicRelease"/></owl:unionOf></owl:Class> </rdfs:domain> <rdfs:range rdf:resource="&xsd;integer"/> <arl:potentialRLRewardMetric rdf:datatype="&xsd;string">Direct reward for promotion/A&amp;R</arl:potentialRLRewardMetric> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;syncUsageDetails"> <rdfs:label xml:lang="en">sync usage details</rdfs:label> <rdfs:comment xml:lang="en">Describes how the music will be used in the visual media.</rdfs:comment> <rdfs:domain rdf:resource="&arl;SynchronizationLicense"/> <rdfs:range rdf:resource="&xsd;string"/> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;teamMemberRole"> <rdfs:label xml:lang="en">team member role</rdfs:label> <rdfs:comment xml:lang="en">The specific role of a TeamMember (e.g., 'Personal Manager', 'Attorney').</rdfs:comment> <rdfs:domain rdf:resource="&arl;TeamMember"/> <rdfs:range rdf:resource="&xsd;string"/> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;terminationWindowEndDate"> <rdfs:label xml:lang="en">termination window end date</rdfs:label> <rdfs:comment xml:lang="en">The end date of the window during which a copyright termination notice can be sent.</rdfs:comment> <rdfs:domain rdf:resource="&arl;TerminationRightProvision"/> <rdfs:range rdf:resource="&xsd;date"/> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;terminationWindowStartDate"> <rdfs:label xml:lang="en">termination window start date</rdfs:label> <rdfs:comment xml:lang="en">The start date of the window during which a copyright termination notice can be sent. Passman p.348.</rdfs:comment> <rdfs:domain rdf:resource="&arl;TerminationRightProvision"/> <rdfs:range rdf:resource="&xsd;date"/> </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="&arl;thresholdMetric">
        <rdfs:label xml:lang="en">threshold metric</rdfs:label>
        <rdfs:comment xml:lang="en">The specific metric type for which a threshold is defined (e.g., 'album_sales', 'monthly_streams').</rdfs:comment>
