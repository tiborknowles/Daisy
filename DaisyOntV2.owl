    <owl:DatatypeProperty rdf:about="&arl;buyoutTerms"> <rdfs:label xml:lang="en">buyout terms</rdfs:label> <rdfs:comment xml:lang="en">Details of a buyout provision for a leaving member of a MusicGroup. Passman p.370.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MusicGroupInternalAgreement"/> <rdfs:range rdf:resource="&xsd;string"/> </owl:DatatypeProperty>
