    <owl:DatatypeProperty rdf:about="&arl;campaignROI"> <rdfs:label xml:lang="en">campaign ROI</rdfs:label> <rdfs:comment xml:lang="en">Calculated Return on Investment for a Marketing Campaign.</rdfs:comment> <rdfs:domain rdf:resource="&arl;MarketingCampaign"/> <rdfs:range rdf:resource="&xsd;decimal"/> <arl:potentialRLRewardMetric rdf:datatype="&xsd;string">Direct reward</arl:potentialRLRewardMetric> </owl:DatatypeProperty>
