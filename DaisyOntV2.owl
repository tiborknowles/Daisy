    <owl:DatatypeProperty rdf:about="&arl;venueCapacity"> <rdfs:label xml:lang="en">venue capacity</rdfs:label> <rdfs:domain rdf:resource="&arl;Venue"/> <rdfs:range rdf:resource="&xsd;integer"/> </owl:DatatypeProperty>
