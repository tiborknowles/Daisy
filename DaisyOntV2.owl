    <owl:Class rdf:about="&arl;ContractualProvision"> <rdfs:label xml:lang="en">Contractual Provision</rdfs:label> <rdfs:comment xml:lang="en">A specific clause or term within a contract or agreement.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;Genre"> <rdfs:label xml:lang="en">Genre</rdfs:label> <rdfs:comment xml:lang="en">A musical genre.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;PublishingSplit"> <rdfs:label xml:lang="en">Publishing Split</rdfs:label> <rdfs:comment xml:lang="en">Represents the division of publishing ownership and income for a MusicWork.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;Territory"> <rdfs:label xml:lang="en">Territory</rdfs:label> <rdfs:comment xml:lang="en">A geographical region relevant to contracts and rights.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;DealTerm"> <rdfs:label xml:lang="en">Deal Term</rdfs:label> <rdfs:comment xml:lang="en">A specific period (initial or option) within a contract, often with its own deliverables and financial terms.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;MediaType"> <rdfs:label xml:lang="en">Media Type</rdfs:label> <rdfs:comment xml:lang="en">The type of media in which music is used (e.g., Film, TV, Game).</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;CopyrightRegistration"> <rdfs:label xml:lang="en">Copyright Registration</rdfs:label> <rdfs:comment xml:lang="en">Represents the formal registration of a copyright.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;AncillaryIncomeStream"> <rdfs:label xml:lang="en">Ancillary Income Stream</rdfs:label> <rdfs:comment xml:lang="en">Non-record income streams like touring, merchandise, sponsorships, often part of 360 deals. Passman p.91.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;Ticket"> <rdfs:label xml:lang="en">Ticket</rdfs:label> <rdfs:comment xml:lang="en">A ticket for an event, such as a concert.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;MusicReleaseFormat"> <rdfs:label xml:lang="en">Music Release Format</rdfs:label> <rdfs:comment xml:lang="en">The physical or digital format of a music release (e.g., Vinyl, CD, Digital Stream).</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;LicenseClause"> <rdfs:subClassOf rdf:resource="&arl;ContractualProvision"/> <rdfs:label xml:lang="en">License Clause</rdfs:label> <rdfs:comment xml:lang="en">A specific clause within a LicenseAgreement.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;IncomeSourceType"> <rdfs:label xml:lang="en">Income Source Type</rdfs:label> <rdfs:comment xml:lang="en">Categorization of income sources, e.g., mechanical, performance, sync. Passman p.190.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;DeliveryRequirement"> <rdfs:label xml:lang="en">Delivery Requirement</rdfs:label> <rdfs:comment xml:lang="en">Specifies the product to be delivered under a contract term. Passman p.115.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;PerformanceMetricThreshold"> <rdfs:label xml:lang="en">Performance Metric Threshold</rdfs:label> <rdfs:comment xml:lang="en">A threshold based on a performance metric (e.g., sales, streams) that can trigger contractual changes like royalty escalations. Passman p.97.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;AandRDecisionParameters"> <rdfs:label xml:lang="en">A&amp;R Decision Parameters</rdfs:label> <rdfs:comment xml:lang="en">Parameters used by A&amp;R agents for making decisions, e.g., signing an artist.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;KPI"> <rdfs:subClassOf rdf:resource="&arl;FinancialConcept"/> <rdfs:label xml:lang="en">Key Performance Indicator (KPI)</rdfs:label> <rdfs:comment xml:lang="en">A measurable value that demonstrates how effectively a company is achieving key business objectives.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;PerformanceMetric"> <rdfs:subClassOf rdf:resource="&arl;MarketingConcept"/> <rdfs:label xml:lang="en">Performance Metric</rdfs:label> <rdfs:comment xml:lang="en">A specific metric used to track performance, often associated with marketing or artist development.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;ArtistCareerState"> <rdfs:label xml:lang="en">Artist Career State</rdfs:label> <rdfs:comment xml:lang="en">A snapshot of an artist's career status at a point in time, used for simulation and RL.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;MusicReleaseLifecycleState"> <rdfs:label xml:lang="en">Music Release Lifecycle State</rdfs:label> <rdfs:comment xml:lang="en">The current phase of a music release (e.g., pre-release, active, catalog).</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;SimulationRun"> <rdfs:label xml:lang="en">Simulation Run</rdfs:label> <rdfs:comment xml:lang="en">Represents a specific execution of a what-if scenario using the digital twin.</rdfs:comment></owl:Class>
    <owl:Class rdf:about="&arl;MonetaryAmount"> <rdfs:label xml:lang="en">Monetary Amount</rdfs:label> <rdfs:comment xml:lang="en">Represents a quantity of money, with a value and a currency.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;CurrencyCode"> <rdfs:label xml:lang="en">Currency Code</rdfs:label> <rdfs:comment xml:lang="en">Represents a currency (e.g., USD, EUR, JPY).</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;ActionStatusCode"> <rdfs:label xml:lang="en">Action Status Code</rdfs:label> <rdfs:comment xml:lang="en">A controlled vocabulary for the status of an AgentAction.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;HeatLevelValue"> <rdfs:label xml:lang="en">Heat Level Value</rdfs:label> <rdfs:comment xml:lang="en">A controlled vocabulary for an Artist's heat level.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;DeliveryStandardValue"> <rdfs:label xml:lang="en">Delivery Standard Value</rdfs:label> <rdfs:comment xml:lang="en">A controlled vocabulary for delivery standards (e.g., Commercially Satisfactory).</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;LaunchMarketingCampaignParameters"> <rdfs:label xml:lang="en">Launch Marketing Campaign Parameters</rdfs:label> <rdfs:comment xml:lang="en">Structured parameters for the LaunchMarketingCampaignAction.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;LaunchMarketingCampaignOutcome"> <rdfs:label xml:lang="en">Launch Marketing Campaign Outcome</rdfs:label> <rdfs:comment xml:lang="en">Structured outcome of the LaunchMarketingCampaignAction.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;ScoutArtistParameters"> <rdfs:label xml:lang="en">Scout Artist Parameters</rdfs:label> <rdfs:comment xml:lang="en">Structured parameters for the ScoutArtistAction.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;ScoutArtistOutcome"> <rdfs:label xml:lang="en">Scout Artist Outcome</rdfs:label> <rdfs:comment xml:lang="en">Structured outcome of the ScoutArtistAction, may include a list of potential artists or a detailed report on one.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;InitiateRoyaltyPaymentParameters"> <rdfs:label xml:lang="en">Initiate Royalty Payment Parameters</rdfs:label> <rdfs:comment xml:lang="en">Structured parameters for initiating a royalty payment.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;InitiateRoyaltyPaymentOutcome"> <rdfs:label xml:lang="en">Initiate Royalty Payment Outcome</rdfs:label> <rdfs:comment xml:lang="en">Structured outcome of initiating a royalty payment, e.g., payment confirmation.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;ExecuteSimulationScenarioParameters"> <rdfs:label xml:lang="en">Execute Simulation Scenario Parameters</rdfs:label> <rdfs:comment xml:lang="en">Structured parameters for executing a simulation scenario.</rdfs:comment> </owl:Class>
    <owl:Class rdf:about="&arl;ExecuteSimulationScenarioOutcome"> <rdfs:label xml:lang="en">Execute Simulation Scenario Outcome</rdfs:label> <rdfs:comment xml:lang="en">Structured outcome of a simulation scenario, including predicted KPIs.</rdfs:comment> </owl:Class>
    
