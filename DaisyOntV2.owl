    <owl:ObjectProperty rdf:about="&arl;parametersForDecisionAction"> <rdfs:label xml:lang="en">parameters for decision action</rdfs:label> <rdfs:comment xml:lang="en">Relates AandRDecisionParameters to the specific SignArtistDecisionAction they inform. Inverse of hasDecisionParameters.</rdfs:comment> <rdfs:domain rdf:resource="&arl;AandRDecisionParameters"/> <rdfs:range rdf:resource="&arl;SignArtistDecisionAction"/> <owl:inverseOf rdf:resource="&arl;hasDecisionParameters"/> </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="&arl;simulationContextOf"> <rdfs:label xml:lang="en">simulation context of</rdfs:label> <rdfs:comment xml:lang="en">Links a simulated entity to the SimulationRun it belongs to, distinguishing it from real-world data.</rdfs:comment> <rdfs:domain rdf:resource="&owl;Thing"/> <rdfs:range rdf:resource="&arl;SimulationRun"/> </owl:ObjectProperty>
     <owl:ObjectProperty rdf:about="&arl;hasSimulatedAction"> <rdfs:label xml:lang="en">has simulated action</rdfs:label> <rdfs:comment xml:lang="en">Links a SimulationRun to the AgentActions that were part of that simulation.</rdfs:comment> <rdfs:domain rdf:resource="&arl;SimulationRun"/> <rdfs:range rdf:resource="&arl;AgentAction"/> </owl:ObjectProperty>
     <owl:ObjectProperty rdf:about="&arl;predictedOutcomeForSimulation"> <rdfs:label xml:lang="en">predicted outcome for simulation</rdfs:label> <rdfs:comment xml:lang="en">Links a SimulationRun to the KPIs or PerformanceMetrics predicted as its outcome.</rdfs:comment> <rdfs:domain rdf:resource="&arl;SimulationRun"/> <rdfs:range> <owl:Class><owl:unionOf rdf:parseType="Collection"><rdf:Description rdf:about="&arl;KPI"/><rdf:Description rdf:about="&arl;PerformanceMetric"/></owl:unionOf></owl:Class> </rdfs:range> </owl:ObjectProperty>
